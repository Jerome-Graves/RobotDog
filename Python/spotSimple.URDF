  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.18 0.09 0.02"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.01"/>
      <material name="yellow"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.18 0.09 0.02"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.01"/>
    </collision>
    <inertial>
      <mass value="100"/>
      <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
  </link>





